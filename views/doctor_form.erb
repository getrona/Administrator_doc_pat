<form action="/doctor_form" method="post">
  <div class="form-group">
    <label for="name_input">Doctor Name</label>
    <input type="text" id="name_input" name="name_input" class="form-control">
  </div>
  <div class="form-group">
    <label for="specialty_input">Doctor Specialty</label>
